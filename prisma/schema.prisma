generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Token {
  id          Int     @id @default(autoincrement())
  title       String? @db.VarChar(255)
  artist      String? @db.VarChar(255)
  image       Bytes
  spotify_uri String? @db.VarChar(255)
}
